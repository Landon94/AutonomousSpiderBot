<?xml version="1.0"?>
<robot name="quad_12servo">

  <!-- ========== "Constants" (edit these numbers) ========== -->
  <!-- Body dimensions (meters) -->
  <!-- BODY_L = 0.30, BODY_W = 0.20, BODY_H = 0.05 -->
  <!-- Leg segment lengths (meters) -->
  <!-- THIGH_L = 0.12, SHIN_L = 0.12 -->

  <!-- Simple materials -->
  <material name="gray"><color rgba="0.6 0.6 0.6 1.0"/></material>
  <material name="black"><color rgba="0.1 0.1 0.1 1.0"/></material>

  <!-- ========================= BASE / BODY ========================= -->
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="2.0"/>
      <!-- Approx inertia for box 0.30 x 0.20 x 0.05, mass 2.0 -->
      <inertia ixx="0.007083" ixy="0" ixz="0"
               iyy="0.015417" iyz="0"
               izz="0.021667"/>
    </inertial>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.30 0.20 0.05"/>
      </geometry>
      <material name="gray"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.30 0.20 0.05"/>
      </geometry>
    </collision>
  </link>

  <!-- Helper note:
       Coordinate convention here:
       +X forward, +Y left, +Z up
       Legs attach on the bottom of the body (z = -BODY_H/2 = -0.025)
  -->

  <!-- ========================= LEG: FRONT LEFT ========================= -->
  <joint name="fl_hip_yaw" type="revolute">
    <parent link="base_link"/>
    <child link="fl_hip_link"/>
    <!-- x=+0.15, y=+0.10, z=-0.025 (corner bottom) -->
    <origin xyz="0.15 0.10 -0.025" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.5708" upper="1.5708" effort="6.0" velocity="6.0"/>
  </joint>

  <link name="fl_hip_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.05"/>
      <!-- small cylinder-ish inertia -->
      <inertia ixx="0.000006667" ixy="0" ixz="0"
               iyy="0.000006667" iyz="0"
               izz="0.000010000"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.02" length="0.02"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.02" length="0.02"/>
      </geometry>
    </collision>
  </link>

  <joint name="fl_hip_pitch" type="revolute">
    <parent link="fl_hip_link"/>
    <child link="fl_thigh_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <!-- Pitch about Y: swings leg forward/back in X-Z -->
    <axis xyz="0 1 0"/>
    <limit lower="-1.2217" upper="1.2217" effort="8.0" velocity="8.0"/>
  </joint>

  <link name="fl_thigh_link">
    <inertial>
      <!-- COM is halfway down the thigh -->
      <origin xyz="0 0 -0.06" rpy="0 0 0"/>
      <mass value="0.15"/>
      <inertia ixx="0.000188438" ixy="0" ixz="0"
               iyy="0.000188438" iyz="0"
               izz="0.000016875"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.06" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.015" length="0.12"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.06" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.015" length="0.12"/>
      </geometry>
    </collision>
  </link>

  <joint name="fl_knee_pitch" type="revolute">
    <parent link="fl_thigh_link"/>
    <child link="fl_shin_link"/>
    <!-- At end of thigh -->
    <origin xyz="0 0 -0.12" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <!-- Knee usually bends "backwards": 0..~135deg -->
    <limit lower="0.0" upper="2.3562" effort="10.0" velocity="10.0"/>
  </joint>

  <link name="fl_shin_link">
    <inertial>
      <origin xyz="0 0 -0.06" rpy="0 0 0"/>
      <mass value="0.12"/>
      <inertia ixx="0.000149070" ixy="0" ixz="0"
               iyy="0.000149070" iyz="0"
               izz="0.000010140"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.06" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.013" length="0.12"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.06" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.013" length="0.12"/>
      </geometry>
    </collision>
  </link>

  <joint name="fl_foot_fixed" type="fixed">
    <parent link="fl_shin_link"/>
    <child link="fl_foot_link"/>
    <origin xyz="0 0 -0.12" rpy="0 0 0"/>
  </joint>

  <link name="fl_foot_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.03"/>
      <!-- sphere inertia approx: 2/5 m r^2, r=0.02 -->
      <inertia ixx="0.0000048" ixy="0" ixz="0"
               iyy="0.0000048" iyz="0"
               izz="0.0000048"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.02"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.02"/>
      </geometry>
    </collision>
  </link>

  <!-- ========================= LEG: FRONT RIGHT ========================= -->
  <joint name="fr_hip_yaw" type="revolute">
    <parent link="base_link"/>
    <child link="fr_hip_link"/>
    <origin xyz="0.15 -0.10 -0.025" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.5708" upper="1.5708" effort="6.0" velocity="6.0"/>
  </joint>

  <link name="fr_hip_link">
    <inertial><origin xyz="0 0 0"/><mass value="0.05"/>
      <inertia ixx="0.000006667" ixy="0" ixz="0" iyy="0.000006667" iyz="0" izz="0.000010000"/>
    </inertial>
    <visual><geometry><cylinder radius="0.02" length="0.02"/></geometry><material name="black"/></visual>
    <collision><geometry><cylinder radius="0.02" length="0.02"/></geometry></collision>
  </link>

  <joint name="fr_hip_pitch" type="revolute">
    <parent link="fr_hip_link"/>
    <child link="fr_thigh_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.2217" upper="1.2217" effort="8.0" velocity="8.0"/>
  </joint>

  <link name="fr_thigh_link">
    <inertial><origin xyz="0 0 -0.06"/><mass value="0.15"/>
      <inertia ixx="0.000188438" ixy="0" ixz="0" iyy="0.000188438" iyz="0" izz="0.000016875"/>
    </inertial>
    <visual><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.015" length="0.12"/></geometry><material name="black"/></visual>
    <collision><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.015" length="0.12"/></geometry></collision>
  </link>

  <joint name="fr_knee_pitch" type="revolute">
    <parent link="fr_thigh_link"/>
    <child link="fr_shin_link"/>
    <origin xyz="0 0 -0.12" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0.0" upper="2.3562" effort="10.0" velocity="10.0"/>
  </joint>

  <link name="fr_shin_link">
    <inertial><origin xyz="0 0 -0.06"/><mass value="0.12"/>
      <inertia ixx="0.000149070" ixy="0" ixz="0" iyy="0.000149070" iyz="0" izz="0.000010140"/>
    </inertial>
    <visual><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.013" length="0.12"/></geometry><material name="black"/></visual>
    <collision><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.013" length="0.12"/></geometry></collision>
  </link>

  <joint name="fr_foot_fixed" type="fixed">
    <parent link="fr_shin_link"/>
    <child link="fr_foot_link"/>
    <origin xyz="0 0 -0.12" rpy="0 0 0"/>
  </joint>

  <link name="fr_foot_link">
    <inertial><origin xyz="0 0 0"/><mass value="0.03"/>
      <inertia ixx="0.0000048" ixy="0" ixz="0" iyy="0.0000048" iyz="0" izz="0.0000048"/>
    </inertial>
    <visual><geometry><sphere radius="0.02"/></geometry><material name="black"/></visual>
    <collision><geometry><sphere radius="0.02"/></geometry></collision>
  </link>

  <!-- ========================= LEG: REAR LEFT ========================= -->
  <joint name="rl_hip_yaw" type="revolute">
    <parent link="base_link"/>
    <child link="rl_hip_link"/>
    <origin xyz="-0.15 0.10 -0.025" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.5708" upper="1.5708" effort="6.0" velocity="6.0"/>
  </joint>

  <link name="rl_hip_link">
    <inertial><origin xyz="0 0 0"/><mass value="0.05"/>
      <inertia ixx="0.000006667" ixy="0" ixz="0" iyy="0.000006667" iyz="0" izz="0.000010000"/>
    </inertial>
    <visual><geometry><cylinder radius="0.02" length="0.02"/></geometry><material name="black"/></visual>
    <collision><geometry><cylinder radius="0.02" length="0.02"/></geometry></collision>
  </link>

  <joint name="rl_hip_pitch" type="revolute">
    <parent link="rl_hip_link"/>
    <child link="rl_thigh_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.2217" upper="1.2217" effort="8.0" velocity="8.0"/>
  </joint>

  <link name="rl_thigh_link">
    <inertial><origin xyz="0 0 -0.06"/><mass value="0.15"/>
      <inertia ixx="0.000188438" ixy="0" ixz="0" iyy="0.000188438" iyz="0" izz="0.000016875"/>
    </inertial>
    <visual><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.015" length="0.12"/></geometry><material name="black"/></visual>
    <collision><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.015" length="0.12"/></geometry></collision>
  </link>

  <joint name="rl_knee_pitch" type="revolute">
    <parent link="rl_thigh_link"/>
    <child link="rl_shin_link"/>
    <origin xyz="0 0 -0.12" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0.0" upper="2.3562" effort="10.0" velocity="10.0"/>
  </joint>

  <link name="rl_shin_link">
    <inertial><origin xyz="0 0 -0.06"/><mass value="0.12"/>
      <inertia ixx="0.000149070" ixy="0" ixz="0" iyy="0.000149070" iyz="0" izz="0.000010140"/>
    </inertial>
    <visual><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.013" length="0.12"/></geometry><material name="black"/></visual>
    <collision><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.013" length="0.12"/></geometry></collision>
  </link>

  <joint name="rl_foot_fixed" type="fixed">
    <parent link="rl_shin_link"/>
    <child link="rl_foot_link"/>
    <origin xyz="0 0 -0.12" rpy="0 0 0"/>
  </joint>

  <link name="rl_foot_link">
    <inertial><origin xyz="0 0 0"/><mass value="0.03"/>
      <inertia ixx="0.0000048" ixy="0" ixz="0" iyy="0.0000048" iyz="0" izz="0.0000048"/>
    </inertial>
    <visual><geometry><sphere radius="0.02"/></geometry><material name="black"/></visual>
    <collision><geometry><sphere radius="0.02"/></geometry></collision>
  </link>

  <!-- ========================= LEG: REAR RIGHT ========================= -->
  <joint name="rr_hip_yaw" type="revolute">
    <parent link="base_link"/>
    <child link="rr_hip_link"/>
    <origin xyz="-0.15 -0.10 -0.025" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.5708" upper="1.5708" effort="6.0" velocity="6.0"/>
  </joint>

  <link name="rr_hip_link">
    <inertial><origin xyz="0 0 0"/><mass value="0.05"/>
      <inertia ixx="0.000006667" ixy="0" ixz="0" iyy="0.000006667" iyz="0" izz="0.000010000"/>
    </inertial>
    <visual><geometry><cylinder radius="0.02" length="0.02"/></geometry><material name="black"/></visual>
    <collision><geometry><cylinder radius="0.02" length="0.02"/></geometry></collision>
  </link>

  <joint name="rr_hip_pitch" type="revolute">
    <parent link="rr_hip_link"/>
    <child link="rr_thigh_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.2217" upper="1.2217" effort="8.0" velocity="8.0"/>
  </joint>

  <link name="rr_thigh_link">
    <inertial><origin xyz="0 0 -0.06"/><mass value="0.15"/>
      <inertia ixx="0.000188438" ixy="0" ixz="0" iyy="0.000188438" iyz="0" izz="0.000016875"/>
    </inertial>
    <visual><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.015" length="0.12"/></geometry><material name="black"/></visual>
    <collision><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.015" length="0.12"/></geometry></collision>
  </link>

  <joint name="rr_knee_pitch" type="revolute">
    <parent link="rr_thigh_link"/>
    <child link="rr_shin_link"/>
    <origin xyz="0 0 -0.12" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0.0" upper="2.3562" effort="10.0" velocity="10.0"/>
  </joint>

  <link name="rr_shin_link">
    <inertial><origin xyz="0 0 -0.06"/><mass value="0.12"/>
      <inertia ixx="0.000149070" ixy="0" ixz="0" iyy="0.000149070" iyz="0" izz="0.000010140"/>
    </inertial>
    <visual><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.013" length="0.12"/></geometry><material name="black"/></visual>
    <collision><origin xyz="0 0 -0.06"/><geometry><cylinder radius="0.013" length="0.12"/></geometry></collision>
  </link>

  <joint name="rr_foot_fixed" type="fixed">
    <parent link="rr_shin_link"/>
    <child link="rr_foot_link"/>
    <origin xyz="0 0 -0.12" rpy="0 0 0"/>
  </joint>

  <link name="rr_foot_link">
    <inertial><origin xyz="0 0 0"/><mass value="0.03"/>
      <inertia ixx="0.0000048" ixy="0" ixz="0" iyy="0.0000048" iyz="0" izz="0.0000048"/>
    </inertial>
    <visual><geometry><sphere radius="0.02"/></geometry><material name="black"/></visual>
    <collision><geometry><sphere radius="0.02"/></geometry></collision>
  </link>

</robot>
